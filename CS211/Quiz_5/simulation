#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5615c0dab6b0 .scope module, "TestBench" "TestBench" 2 4;
 .timescale -9 -12;
v0x5615c0dc4a10_0 .var "A", 0 0;
v0x5615c0dc4ad0_0 .net "A_1", 0 0, L_0x5615c0dc4ce0;  1 drivers
v0x5615c0dc4b70_0 .var "B", 0 0;
v0x5615c0dc4c10_0 .net "B_1", 0 0, L_0x5615c0dc4eb0;  1 drivers
S_0x5615c0dab840 .scope module, "DUT0" "Main" 2 14, 3 55 0, S_0x5615c0dab6b0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "A_1";
    .port_info 1 /OUTPUT 1 "B_1";
    .port_info 2 /INPUT 1 "A";
    .port_info 3 /INPUT 1 "B";
L_0x5615c0dc4ce0 .functor BUFZ 1, v0x5615c0dc4b70_0, C4<0>, C4<0>, C4<0>;
L_0x5615c0dc4df0 .functor OR 1, L_0x5615c0dc4fc0, v0x5615c0dc4a10_0, C4<0>, C4<0>;
L_0x5615c0dc4eb0 .functor BUFZ 1, L_0x5615c0dc4df0, C4<0>, C4<0>, C4<0>;
L_0x5615c0dc4fc0 .functor NOT 1, v0x5615c0dc4a10_0, C4<0>, C4<0>, C4<0>;
v0x5615c0daba40_0 .net "A", 0 0, v0x5615c0dc4a10_0;  1 drivers
v0x5615c0dc45a0_0 .net "A_1", 0 0, L_0x5615c0dc4ce0;  alias, 1 drivers
v0x5615c0dc4660_0 .net "B", 0 0, v0x5615c0dc4b70_0;  1 drivers
v0x5615c0dc4700_0 .net "B_1", 0 0, L_0x5615c0dc4eb0;  alias, 1 drivers
v0x5615c0dc47c0_0 .net "not_0_out", 0 0, L_0x5615c0dc4fc0;  1 drivers
v0x5615c0dc48d0_0 .net "or_0_out", 0 0, L_0x5615c0dc4df0;  1 drivers
    .scope S_0x5615c0dab6b0;
T_0 ;
    %vpi_call 2 19 "$display", "Testing all input combinations..." {0 0 0};
    %vpi_call 2 20 "$display", "A B | A_1 B_1 | Expected A_1 B_1" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5615c0dc4a10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5615c0dc4b70_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 25 "$display", "%b %b |  %b   %b  | 0 1", v0x5615c0dc4a10_0, v0x5615c0dc4b70_0, v0x5615c0dc4ad0_0, v0x5615c0dc4c10_0 {0 0 0};
    %load/vec4 v0x5615c0dc4ad0_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %flag_mov 8, 6;
    %load/vec4 v0x5615c0dc4c10_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_or 6, 8;
    %jmp/0xz  T_0.0, 6;
    %vpi_call 2 26 "$display", "Error: Test case 1 failed!" {0 0 0};
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5615c0dc4a10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5615c0dc4b70_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 31 "$display", "%b %b |  %b   %b  | 1 1", v0x5615c0dc4a10_0, v0x5615c0dc4b70_0, v0x5615c0dc4ad0_0, v0x5615c0dc4c10_0 {0 0 0};
    %load/vec4 v0x5615c0dc4ad0_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_mov 8, 6;
    %load/vec4 v0x5615c0dc4c10_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_or 6, 8;
    %jmp/0xz  T_0.2, 6;
    %vpi_call 2 32 "$display", "Error: Test case 2 failed!" {0 0 0};
T_0.2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5615c0dc4a10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5615c0dc4b70_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 37 "$display", "%b %b |  %b   %b  | 0 1", v0x5615c0dc4a10_0, v0x5615c0dc4b70_0, v0x5615c0dc4ad0_0, v0x5615c0dc4c10_0 {0 0 0};
    %load/vec4 v0x5615c0dc4ad0_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %flag_mov 8, 6;
    %load/vec4 v0x5615c0dc4c10_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_or 6, 8;
    %jmp/0xz  T_0.4, 6;
    %vpi_call 2 38 "$display", "Error: Test case 3 failed!" {0 0 0};
T_0.4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5615c0dc4a10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5615c0dc4b70_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 43 "$display", "%b %b |  %b   %b  | 1 1", v0x5615c0dc4a10_0, v0x5615c0dc4b70_0, v0x5615c0dc4ad0_0, v0x5615c0dc4c10_0 {0 0 0};
    %load/vec4 v0x5615c0dc4ad0_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_mov 8, 6;
    %load/vec4 v0x5615c0dc4c10_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
